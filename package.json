{
  "name": "@oddjs/odd",
  "version": "0.40.0-prerelease",
  "description": "ODD SDK",
  "keywords": [
    "WebCrypto",
    "auth",
    "files",
    "distributed",
    "DAG",
    "DID",
    "IPFS",
    "IPLD",
    "UCAN",
    "WNFS"
  ],
  "type": "module",
  "main": "lib/index.js",
  "exports": {
    ".": "./lib/index.js",
    "./package.json": "./package.json",
    "./lib/*": [
      "./lib/*.js",
      "./lib/*",
      "./lib/*/index.js"
    ],
    "./*": [
      "./lib/*.js",
      "./lib/*",
      "./lib/*/index.js",
      "./*"
    ]
  },
  "types": "lib/index.d.ts",
  "typesVersions": {
    "*": {
      "lib/index.d.ts": [
        "lib/index.d.ts"
      ],
      "*": [
        "lib/*"
      ]
    }
  },
  "files": [
    "lib",
    "dist",
    "docs",
    "src",
    "README.md",
    "CHANGELOG.md",
    "LICENSE",
    "package.json",
    "!*.test.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/oddsdk/ts-odd"
  },
  "homepage": "https://odd.dev",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "docs": "rimraf docs && typedoc",
    "lint": "eslint src/**/*.ts src/*.ts tests/**/*.ts tests/*.ts",
    "prebuild": "rimraf lib dist && node scripts/gen-version.js",
    "build": "tsc && npm run build:minified",
    "build:minified": "node scripts/build-minified.js",
    "format": "dprint fmt",
    "start": "tsc -w",
    "test": "mocha",
    "test:gh-action": "TEST_ENV=gh-action npm run test",
    "test:imports": "madge src --ts-config tsconfig.json --extensions ts --circular --warning",
    "test:prod": "npm run build && npm run lint && npm run test:imports && npm run test:gh-action",
    "test:unit": "mocha --watch --testPathPattern=src/",
    "test:wasm": "WNFS_WASM=true mocha",
    "prepare": "npm run build && npm run docs",
    "publish-dry": "npm publish --dry-run",
    "publish-alpha": "npm publish --tag alpha",
    "publish-latest": "npm publish --tag latest"
  },
  "dependencies": {
    "@ipld/dag-cbor": "^9.0.4",
    "@ipld/dag-pb": "^4.0.5",
    "debounce-promise": "^3.1.2",
    "emittery": "^1.0.1",
    "interface-blockstore": "^5.2.5",
    "ipfs-unixfs": "^11.1.0",
    "ipfs-unixfs-exporter": "^13.2.1",
    "ipfs-unixfs-importer": "^15.2.1",
    "iso-base": "^2.0.0",
    "iso-did": "^1.3.2",
    "iso-signatures": "^0.1.9",
    "it-all": "^3.0.3",
    "multiformats": "^12.0.1",
    "uint8arrays": "^4.0.6",
    "wnfs": "0.1.26"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@ipld/car": "^5.2.2",
    "@types/debounce-promise": "^3.1.6",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.5.7",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "assert": "^2.0.0",
    "copyfiles": "^2.4.1",
    "dprint": "^0.40.2",
    "esbuild": "^0.19.2",
    "esbuild-plugin-wasm": "^1.1.0",
    "eslint": "^8.48.0",
    "fast-check": "^3.12.0",
    "globby": "^13.2.2",
    "madge": "^6.1.0",
    "mocha": "^10.2.0",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typedoc": "^0.25.0",
    "typedoc-plugin-missing-exports": "^2.1.0",
    "typedoc-plugin-rename-defaults": "^0.6.6",
    "typescript": "^5.2.2",
    "util": "^0.12.4"
  },
  "peerDependencies": {
    "@chainsafe/libp2p-noise": "^13.0.0",
    "@chainsafe/libp2p-yamux": "^5.0.0",
    "@libp2p/mplex": "^9.0.5",
    "@libp2p/peer-id": "^3.0.2",
    "@libp2p/websockets": "^7.0.5",
    "@libp2p/webtransport": "^3.0.7",
    "@multiformats/multiaddr": "^12.1.7",
    "@noble/ciphers": "^0.3.0",
    "@noble/curves": "^1.2.0",
    "@noble/hashes": "^1.3.2",
    "@ucans/core": "^0.12.0",
    "blockstore-level": "^1.1.3",
    "ipfs-bitswap": "^19.0.0",
    "libp2p": "^0.46.7",
    "localforage": "^1.10.0"
  }
}
