{
  "name": "webnative",
  "version": "0.30.0-alpha12",
  "description": "Fission Webnative SDK",
  "keywords": [
    "WebCrypto",
    "auth",
    "IPFS",
    "files"
  ],
  "type": "module",
  "main": "lib/index.js",
  "exports": {
    ".": "./lib/index.js",
    "./lib/*": "./lib/*",
    "./*": "./lib/*",
    "./package.json": "./package.json"
  },
  "types": "lib/index.d.ts",
  "typesVersions": {
    "*": {
      "lib/index.d.ts": [
        "lib/index.d.ts"
      ],
      "*": [
        "lib/*"
      ]
    }
  },
  "files": [
    "lib",
    "dist",
    "README.md",
    "CHANGELOG.md",
    "LICENSE",
    "package.json",
    "!*.test.ts",
    "docs"
  ],
  "author": "Daniel Holmgren <daniel@fission.codes>",
  "repository": {
    "type": "git",
    "url": "https://github.com/fission-suite/webnative"
  },
  "homepage": "https://guide.fission.codes",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=15"
  },
  "scripts": {
    "docs": "rimraf docs && typedoc",
    "lint": "yarn eslint src/**/*.ts src/*.ts tests/**/*.ts tests/*.ts",
    "prebuild": "rimraf lib dist && node scripts/gen-version.js",
    "build": "tsc && yarn run build:minified",
    "build:minified": "node scripts/build-minified.js",
    "start": "tsc -w",
    "test": "mocha",
    "test:prod": "yarn run build && yarn run lint && yarn run test:gh-action",
    "test:types": "cp -RT tests/types/ lib/ && yarn run tsd",
    "test:unit": "mocha --watch --testPathPattern=src/",
    "test:gh-action": "TEST_ENV=gh-action yarn test",
    "prepare": "yarn build && yarn docs",
    "publish-dry": "npm publish --dry-run",
    "publish-alpha": "npm publish --tag alpha",
    "publish-latest": "npm publish --tag latest"
  },
  "devDependencies": {
    "@types/expect": "^24.3.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.10",
    "@types/throttle-debounce": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "blockstore-core": "^1.0.5",
    "datastore-core": "^7.0.0",
    "esbuild": "^0.14.11",
    "eslint": "^8.7.0",
    "expect": "^27.4.6",
    "fast-check": "^2.21.0",
    "ipfs-core": "^0.13.0",
    "ipfs-core-types": "^0.9.0",
    "ipfs-repo": "^14.0.0",
    "mocha": "^9.1.4",
    "multihashing-async": "^2.1.4",
    "puppeteer": "^13.1.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typedoc": "^0.22.11",
    "typescript": "^4.5.4",
    "yarn": "^1.22.17"
  },
  "dependencies": {
    "@ipld/dag-cbor": "^7.0.0",
    "@ipld/dag-pb": "^2.1.15",
    "cborg": "^1.6.1",
    "cids": "^1.1.9",
    "fission-bloom-filters": "1.7.1",
    "ipfs-message-port-client": "^0.10.3",
    "ipfs-message-port-protocol": "^0.10.5",
    "ipld-dag-pb": "^0.22.3",
    "keystore-idb": "^0.15.4",
    "localforage": "^1.10.0",
    "multiformats": "^9.5.9",
    "one-webcrypto": "^1.0.3",
    "throttle-debounce": "^3.0.1",
    "tweetnacl": "^1.0.3",
    "uint8arrays": "^3.0.0"
  },
  "tsd": {
    "directory": "lib"
  }
}
